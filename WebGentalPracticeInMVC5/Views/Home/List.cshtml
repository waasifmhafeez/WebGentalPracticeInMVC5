@model IEnumerable<WebGentalPracticeInMVC5.Models.StudentModel>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>list</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Sid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sage)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Sid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sage)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Sid }) |
                @Html.ActionLink("Details", "Details", new { id=item.Sid }) |
                @Html.ActionLink("Delete", "Delete", new {  id=item.Sid })
                @*//<a href="/Home/Delete?id=@item.Sid">Delete</a>*@
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
